{
  "name": "lighthouse-vsts",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "package": "npm run clean && npm run compile && tfx extension create",
    "clean": "rimraf ./*.vsix",
    "compile": "tsc -p task && tsc -p web",
    "postinstall": "npm run install-task-lib && npm run compile",
    "install-task-lib": "cd task && npm install",
    "test": "mocha task/test/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "vss-web-extension-sdk": "^5.141.0"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/node": "^10.17.14",
    "husky": "^4.2.1",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.1",
    "tfx-cli": "^0.7.11",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
